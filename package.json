{
  "name": "@earth-app/moho",
  "description": "An NPM package with a curated list of special events used by The Earth App",
  "version": "1.0.1",
  "author": "earth-app",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "src",
    "docs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "example": "bun run example/example.ts",
    "build": "bun build src/index.ts --outdir=dist --target=node --format=esm --minify --sourcemap=linked && bunx tsc --project tsconfig.build.json",
    "compile": "bunx babel src --out-dir out --extensions \".ts\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "prettier": "bunx prettier --write .",
    "prettier:check": "bunx prettier --check .",
    "prepare": "husky install",
    "doc": "jsdoc out/* -d docs -R README.md"
  },
  "lint-staged": {
    "*.{ts,js,cjs,json,md}": "prettier --write"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-typescript": "^7.28.5",
    "@jest/globals": "^30.2.0",
    "@types/bun": "latest",
    "babel-plugin-add-module-exports": "^1.0.4",
    "docdash": "^2.0.2",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jsdoc": "^4.0.5",
    "jsdoc-babel": "^0.5.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "prettier-plugin-organize-imports": "4.3.0",
    "ts-jest": "^29.4.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "bugs": {
    "url": "https://github.com/earth-app/moho/issues"
  },
  "homepage": "https://github.com/earth-app/moho#readme",
  "keywords": [
    "calendar",
    "dates",
    "events",
    "holidays",
    "birthdays",
    "recurring-events"
  ]
}
